<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<VBox minHeight="768.0" minWidth="1024.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.witype.hfm.Controller">
    <children>
        <HBox alignment="CENTER_RIGHT" minHeight="30.0" prefWidth="1024.0" styleClass="primary">
            <children>
                <StackPane fx:id="settingPane" onMouseClicked="#onSettingClick" prefHeight="30.0" prefWidth="30.0" styleClass="selectable_background_black">
                    <children>
                        <ImageView fitHeight="12.0" fitWidth="12.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../image/ic_window_setting.png" />
                            </image>
                        </ImageView>
                    </children>
                </StackPane>
                <StackPane onMouseClicked="#onAboutClick" prefHeight="30.0" prefWidth="30.0" styleClass="selectable_background_black">
                    <children>
                        <ImageView fitHeight="12.0" fitWidth="12.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../image/ic_window_about.png" />
                            </image>
                        </ImageView>
                    </children>
                </StackPane>
                <StackPane onMouseClicked="#onWindowMinClick" prefHeight="30.0" prefWidth="30.0" styleClass="selectable_background_black">
                    <children>
                        <ImageView fitHeight="12.0" fitWidth="12.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../image/ic_window_min.png" />
                            </image>
                        </ImageView>
                    </children>
                </StackPane>
                <StackPane fx:id="closePane" onMouseClicked="#onCloseClick" prefHeight="30.0" prefWidth="30.0" styleClass="selectable_background_black">
                    <children>
                        <ImageView fitHeight="12.0" fitWidth="12.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../image/ic_window_close.png" />
                            </image>
                        </ImageView>
                    </children>
                </StackPane>
            </children>
        </HBox>
        <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" styleClass="primary">
            <children>
                <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../image/ic_logo_large.png" />
                    </image>
                    <HBox.margin>
                        <Insets left="20.0" />
                    </HBox.margin>
                </ImageView>
            </children>
        </HBox>
      <BorderPane prefHeight="639.0" prefWidth="200.0" styleClass="window_background">
         <top>
            <VBox fx:id="info_content" prefHeight="572.0" prefWidth="1024.0">
               <padding>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </padding>
               <children>
                  <Label styleClass="secondary_text" text="Application" textFill="#757575">
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </Label>
                  <VBox alignment="CENTER" nodeOrientation="LEFT_TO_RIGHT" prefHeight="50.0" prefWidth="200.0" styleClass="infoContentLayout">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                     <children>
                        <GridPane>
                          <columnConstraints>
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                          </columnConstraints>
                          <rowConstraints>
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                          </rowConstraints>
                        </GridPane>
                        <Label styleClass="selectable_text_gray" text="点击添加渠道信息">
                           <VBox.margin>
                              <Insets bottom="5.0" top="5.0" />
                           </VBox.margin></Label>
                     </children>
                  </VBox>
                  <HBox prefHeight="1.0" prefWidth="200.0" styleClass="divider" />
                  <Label styleClass="secondary_text" text="History" textFill="#757575">
                     <VBox.margin>
                        <Insets bottom="10.0" top="10.0" />
                     </VBox.margin>
                  </Label>
                  <HBox prefHeight="50.0" prefWidth="200.0" styleClass="infoContentLayout">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </HBox>
                  <HBox prefHeight="1.0" prefWidth="200.0" styleClass="divider" />
                  <Label styleClass="secondary_text" text="Detail" textFill="#757575">
                     <VBox.margin>
                        <Insets bottom="10.0" top="10.0" />
                     </VBox.margin>
                  </Label>
                  <HBox prefHeight="50.0" prefWidth="200.0" styleClass="infoContentLayout" />
                  <HBox prefHeight="1.0" prefWidth="200.0" styleClass="divider" />
               </children>
            </VBox>
         </top>
         <bottom>
            <HBox alignment="CENTER" prefHeight="50.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <Label alignment="CENTER" contentDisplay="TOP" styleClass="secondary_text" text="Design by Typer" textFill="#757575" />
               </children>
            </HBox>
         </bottom>
      </BorderPane>
    </children>
   <stylesheets>
      <URL value="@../selector/selectors.css" />
      <URL value="@../css/style_title_bar.css" />
   </stylesheets>
</VBox>
